{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://thlgapp.com/conditionSearch.schema.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    },
    "condition": {
      "type": "string",
      "minLength": 1
    }

  },
  "additionalProperties": false,
  "required": [
    "username",
    "password",
    "condition"
  ]
}
